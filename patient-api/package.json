{
  "name": "@fuse/api",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "dev": "pnpm with-env ts-node-dev --respawn src/main.ts",
    "pm2:start": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=1024 --no-warnings' pm2 start dist/main.js -i 1 --name patient-api",
    "pm2:reload": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=1024 --no-warnings' pm2 reload patient-api",
    "pm2:reload-no-build": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=1024 --no-warnings' pm2 reload patient-api",
    "pm2:stop": "pm2 stop patient-api",
    "clean": "git clean -xdf .cache .turbo dist .next node_modules",
    "with-env": "dotenv -e ../.env.local --"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.886.0",
    "@sendgrid/mail": "^8.1.6",
    "@types/jsonwebtoken": "catalog:",
    "@types/multer": "^2.0.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "catalog:",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "catalog:",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sequelize-typescript": "^2.1.6",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^6.0.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "catalog:",
    "dotenv-cli": "catalog:",
    "ts-node-dev": "^2.0.0",
    "typescript": "catalog:"
  }
}
