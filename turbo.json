{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^dev"
      ],
      "env": [

      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "pm2:stop": {
      "cache": false
    },
    "pm2:reload": {
      "cache": false,
      "dependsOn": [
        "build",
        "pm2:stop"
      ]
    }
  },
  "globalDependencies": [
    ".env.local",
    ".env.production"
  ],
  "globalEnv": [
  ]
}