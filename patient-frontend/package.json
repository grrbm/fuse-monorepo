{
  "name": "@fuse/frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm with-env next dev -p 3000 -H 0.0.0.0",
    "build": "NODE_ENV=production pnpm with-env next build",
    "start": "next start",
    "lint": "next lint",
    "print2": "node print-structure.js",
    "print": "node print-structure-with-preview.js",
    "pm2:start": "pnpm with-env pm2 start npm --name patient-frontend --node-args='--max-old-space-size=1024 --no-warnings' -- start",
    "pm2:reload": "pnpm build && pnpm with-env pm2 reload patient-frontend --update-env",
    "pm2:reload-no-build": "pnpm with-env pm2 reload patient-frontend --update-env",
    "pm2:stop": "pm2 stop patient-frontend",
    "clean": "git clean -xdf .cache .turbo dist .next node_modules",
    "with-env": "dotenv -e ../.env.local --"
  },
  "dependencies": {
    "@fuse/enums": "workspace:*",
    "@heroui/react": "^2.8.3",
    "@iconify/react": "^6.0.1",
    "@stripe/react-stripe-js": "^2.8.1",
    "@stripe/stripe-js": "^4.8.0",
    "framer-motion": "^12.23.12",
    "next": "catalog:next14",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:react18",
    "autoprefixer": "catalog:",
    "dotenv-cli": "catalog:",
    "eslint": "catalog:",
    "postcss": "catalog:",
    "tailwindcss": "^3.4.17",
    "typescript": "catalog:"
  }
}